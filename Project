class VendingMachine:
	def __init__(self, item_code, _item, item_amount = 10, _sheckles=0):
		self.itemcode = item_code
		self.item = _item
		self.itemamount = item_amount
		self.sheckles=_sheckles
	def get_code(self):
		return self.itemcode
    
	def get_item(self):
		return self.item
    
	def get_amount(self):
		return self.itemamount
    
	def get_currency(self):
		return self.currency
		
	def set_item(self, other):
		self.item = other.get_item()
	
